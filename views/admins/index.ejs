<% layout("/layouts/boilerplate") %>
<div>
    <div crossorigin="anonymous" class = "mb-3" id="cluster-map"></div>
    <% for(let user of users) {%>
        <div class="card mb-3 ">
            <div class="row">
                <div class="col-md-8">
                    <div class="card-body">
                        <h5 class = "card-title"><%= user.name %></h5>
                        <p class = "card-text"><%= user.email %></p>
                        <p class = "card-text"><%= user.phoneNo %></p>                          
                    </div>
                </div>
            </div>
        </div>
        
    <% } %>

</div>